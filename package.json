{
  "name": "github-proxy",
  "private": true,
  "description": "GitHub proxy on Cloudflare Workers",
  "repository": "https://github.com/mchaNetwork/github-proxy",
  "license": "MIT",
  "author": "mochaaP <npm@mochaa.ws> && Paperdragon <2678885646@qq.com>",
  "type": "module",
  "main": "dist/_worker.js",
  "scripts": {
    "build": "rimraf dist && parcel build",
    "update": "npm update",
    "deploy": "npm run test && npm run build && wrangler pages deploy dist",
    "lint": "eslint .",
    "size": "size-limit",
    "start": "wrangler pages dev dist",
    "test": "npm run size"
  },
  "browserslist": "Firefox ESR, iOS >= 12.5, Chrome >= 87",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "eslint-config-xo": "^0.46.0",
    "eslint-config-xo-typescript": "^7.0.0",
    "nord": "nordtheme/nord",
    "ress": "^5.0.2",
    "worktop": "next"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20241011.0",
    "@size-limit/preset-small-lib": "^11.1.6",
    "eslint-define-config": "^2.1.0",
    "parcel": "2.12.0",
    "rimraf": "^6.0.1",
    "size-limit": "^11.1.6",
    "wrangler": "^3.80.5",
    "xo": "0.59.3"
  },
  "peerDependencies": {
    "typescript": "^5.3"
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true
    }
  },
  "size-limit": [
    {
      "path": "src/_worker.ts",
      "brotli": false
    }
  ],
  "targets": {
    "frontend": {
      "source": [
        "src/index.html",
        "src/404.html"
      ],
      "distDir": "dist/"
    },
    "main": {
      "source": "src/_worker.ts",
      "context": "web-worker",
      "outputFormat": "esmodule",
      "includeNodeModules": true,
      "optimize": true
    }
  }
}
